<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RecipeDef>
    <defName>Make_PrefabPanel</defName>
    <label>make prefab panels</label>
    <description>Stamp and form steel into modular prefab panels for rapid wall construction.</description>
    <jobString>Making prefab panels.</jobString>
    <workAmount>800</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PrefabPanel>1</PrefabPanel>
    </products>
    <skillRequirements>
      <Crafting>4</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <researchPrerequisites>
      <li>PrefabConstruction</li>
    </researchPrerequisites>
  </RecipeDef>

  <RecipeDef>
    <defName>Make_PrefabPanelBulk</defName>
    <label>make prefab panels x10</label>
    <description>Stamp and form a large batch of steel into modular prefab panels for rapid wall construction.</description>
    <jobString>Making prefab panels (bulk).</jobString>
    <workAmount>7200</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <PrefabPanel>10</PrefabPanel>
    </products>
    <skillRequirements>
      <Crafting>4</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>TableMachining</li>
    </recipeUsers>
    <researchPrerequisites>
      <li>PrefabConstruction</li>
    </researchPrerequisites>
  </RecipeDef>

</Defs>
