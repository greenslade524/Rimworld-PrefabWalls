<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <RecipeDef>
    <defName>Make_AdvPrefabPanel</defName>
    <label>make advanced prefab panels</label>
    <description>Fabricate reinforced plasteel-steel panels for advanced modular construction.</description>
    <jobString>Making advanced prefab panels.</jobString>
    <workAmount>1200</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>12</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <AdvPrefabPanel>1</AdvPrefabPanel>
    </products>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>FabricationBench</li>
    </recipeUsers>
    <researchPrerequisites>
      <li>AdvPrefabConstruction</li>
    </researchPrerequisites>
  </RecipeDef>

  <RecipeDef>
    <defName>Make_AdvPrefabPanelBulk</defName>
    <label>make advanced prefab panels x10</label>
    <description>Fabricate a large batch of reinforced plasteel-steel panels for advanced modular construction.</description>
    <jobString>Making advanced prefab panels (bulk).</jobString>
    <workAmount>10800</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <effectWorking>Cook</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>120</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <AdvPrefabPanel>10</AdvPrefabPanel>
    </products>
    <skillRequirements>
      <Crafting>6</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <recipeUsers>
      <li>FabricationBench</li>
    </recipeUsers>
    <researchPrerequisites>
      <li>AdvPrefabConstruction</li>
    </researchPrerequisites>
  </RecipeDef>

</Defs>
