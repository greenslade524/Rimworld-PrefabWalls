<?xml version="1.0" encoding="utf-8"?>
<Patch>

  <!-- Orbital bulk goods trader: standard panels -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>PrefabPanel</thingDef>
        <countRange>10~40</countRange>
      </li>
    </value>
  </Operation>

  <!-- Orbital bulk goods trader: advanced panels -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>AdvPrefabPanel</thingDef>
        <countRange>5~20</countRange>
      </li>
    </value>
  </Operation>

  <!-- Outlander bulk goods caravan: standard panels -->
