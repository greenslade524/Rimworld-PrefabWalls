    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>AdvPrefabPanel</thingDef>
        <countRange>3~12</countRange>
      </li>
    </value>
  </Operation>

  <!-- Outlander settlement base: standard panels -->
