    <xpath>/Defs/TraderKindDef[defName="Base_Outlander_Standard"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>PrefabPanel</thingDef>
        <countRange>5~30</countRange>
      </li>
    </value>
  </Operation>

  <!-- Outlander settlement base: advanced panels -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Base_Outlander_Standard"]/stockGenerators</xpath>
    <value>
      <li Class="StockGenerator_SingleDef">
        <thingDef>AdvPrefabPanel</thingDef>
        <countRange>3~15</countRange>
      </li>
    </value>
  </Operation>

  <!-- Orbital exotic goods trader: advanced panels only -->
  <Operation Class="PatchOperationAdd">
    <xpath>/Defs/TraderKindDef[defName="Orbital_Exotic"]/stockGenerators</xpath>
